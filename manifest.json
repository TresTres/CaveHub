{
  "manifest_version": 2,
  "name": "CaveHub",
  "short_name": "CaveHub",
  "version": "2.0",
  "description": "Lets you know whats goin' on around the cave.",
  "icons": {  "128" : "bigLogo.png" },
  "browser_action": {
    "default_icon": "tiny.png",
    "default_popup": "home.html"
  },
  "background": {
  	"scripts": ["home.js", "jquery-1.11.2.js"]
  },
  "permissions": [
    "<all_urls>"
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"

}
